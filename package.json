    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "native-base": "^2.11.0",
    "normalizr": "^3.3.0",
    "query-string": "^6.2.0",
    "react": "16.6.3",
    "react-native": "0.58.4",
    "react-native-config": "^0.11.7",
    "react-native-gesture-handler": "^1.0.15",
    "react-navigation": "^3.3.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "redux-persist": "^5.10.0",
    "rxjs": "^6.4.0",
    "typesafe-actions": "^3.1.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@types/google__maps": "^0.5.2",
