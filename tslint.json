{
    "defaultSeverity": "error",
    "extends": ["tslint-react", "tslint-config-airbnb"],
    "jsRules": {},
    "rules": {
      "semicolon": [true, "never"],
      "import-name": false,
      "trailing-comma": [
        true,
        {
          "singleline": "never",
          "multiline": {
            "objects": "always",
            "arrays": "always",
            "functions": "never",
            "typeLiterals": "ignore"
          }
        }
      ],
      "no-unused-variable": true,
      "prefer-array-literal": [true, { "allow-type-parameters": true }]
    },
    "rulesDirectory": []
  }
  